  <div class= "container">
    <div class= "row">
      <div class="large-12 columns">

          <% yearbooks.forEach((yearbook) => { %>
            <img src="<%= yearbook.image %>">

            <a href="/yearbooks/<%= yearbook.id %>"><h1><%= yearbook.name %></h1></a>
            <p><%= yearbook.description %></p>
            <% }); %>

        </div>
      </div>
    </div>
